        turquoise = Color( 75, 200, 200 )

        OOC = ( text ) =>
            Jailbreak.SendChatText( false, turquoise, "[#jb.chat.ooc] ", @GetPlayerColor!\ToColor!, @Nick!, color_white, ": " .. text )
        Jailbreak.OOC = OOC

        Jailbreak.SetChatCommand "ooc", ( text ) =>
            if not Enabled\GetBool! and not @IsAdmin! and Jailbreak.GameInProgress!
                @SendNotify( "#jb.chat.fail", NOTIFY_ERROR, 5 )
                return

            OOC( @, text )
