        Jailbreak.SetChatCommand "roll", ( text ) =>
            int1, int2 = string.match( text, "(-?%d+)%s+(-?%d+)" )
            unless int1 and int2
                @SendNotify( "#jb.chat.fail", NOTIFY_ERROR, 5 )
                return

            Me( @, "#jb.chat.rolled " .. math.random( tonumber( int1 ) or 0, tonumber( int2 ) or 0 ) )

    do

