ENT.Type = CLIENT and "anim" or "brush"

ENT.SetupDataTables = =>
    @NetworkVar( "Int", 0, "Team" )
    @NetworkVar( "Bool", 0, "Disabled" )

    if SERVER
        @SetTeam( 0 )
        @SetDisabled( false )

ENT.Initialize = =>
    @SetCustomCollisionCheck( true )

    if SERVER
        @PhysicsInit( SOLID_BSP )
        @SetMoveType( MOVETYPE_NONE )
